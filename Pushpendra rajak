<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
<script>
  // Set the contest deadline (in UTC)
  var deadline = moment.utc("2023-05-31T23:59:00");
  
  // Update the countdown every second
  setInterval(function() {
    var now = moment.utc();
    var duration = moment.duration(deadline.diff(now));
    
    // Update the countdown element with the remaining time
    document.getElementById("countdown").textContent = duration.days() + " days, " + duration.hours() + " hours, " + duration.minutes() + " minutes, " + duration.seconds() + " seconds";
  }, 1000);
</script>
